{
  "mcpServers": {
    "shadcn": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "shadcn@latest",
        "mcp"
      ]
    },
    "puppeteer": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ]
    },
    "shopify-dev-mcp": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@shopify/dev-mcp@latest"
      ]
    }
  }
}
